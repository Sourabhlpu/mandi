<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FindFalconeFragment">


    <fragment
        android:id="@+id/FindFalconeFragment"
        android:name="sourabh.pal.findfalcone.find.presentation.views.FindFalconeFragment"
        android:label="@string/find_falcone_fragment_label"
        tools:layout="@layout/fragment_find_falcone">
        <action
            android:id="@+id/action_FindFalconeFragment_to_SuccessFragment"
            app:destination="@id/success_fragment" />
    </fragment>

    <fragment
        android:id="@+id/success_fragment"
        android:name="sourabh.pal.findfalcone.find.presentation.views.FindFalconeSuccessFragment"
        android:label="@string/falcone_found"
        tools:layout="@layout/fragment_find_falcone_success">

        <action
            android:id="@+id/action_FindFalconeFragment_to_SuccessFragment"
            app:destination="@id/FindFalconeFragment" />
    </fragment>
</navigation>